md-toolbar(class="topBar", md-scroll-shrink)
    div(class="md-toolbar-tools")
        h3(flex)
            span Follow Ups
        md-button(class="md-icon-button", aria-label="Search")
            md-icon search
        md-button(class="md-icon-button", aria-label="Add")
            md-icon add

md-content(flex, layout="column")
    md-list-item(class="md-2-line" ng-repeat="followup in data.followUps.followUps" ng-click="null")
        img(class="md-avatar", src="/images/ic_person_black_48px.svg")
        div(class="md-list-item-text" layout="column")
            h3 {{followup.name}}
            p dob: {{followup.dob | date : 'd MMM yyyy'}}, {{followup.gender}}